                   action="#{effects.resetDisappearEffect}"/>

    </ice:panelGrid>

</ice:panelGroup>
<!-- Shake effects  -->
<ice:panelGroup id="effectShakeSpecMenu">
    <ice:commandButton value="Invoke"

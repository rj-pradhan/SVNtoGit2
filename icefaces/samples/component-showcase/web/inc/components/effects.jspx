        <ice:commandButton value="Shrink"
                           action="#{effects.scale}"/>

        <ice:commandButton value="Enlarge"
                           action="#{effects.resetScale}"/>

    </ice:panelGrid>
</ice:panelGroup>
<!-- Puff effects  -->
<ice:panelGroup id="effectPuffSpecMenu">

    <ice:panelGrid columns="2"

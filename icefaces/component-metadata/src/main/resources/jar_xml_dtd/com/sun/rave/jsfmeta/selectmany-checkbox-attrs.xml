<!-- ========== SelectMany+CheckboxList Attributes ========================= -->


      <attribute>
        <attribute-name>border</attribute-name>
        <attribute-extension>
          <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.property.IntegerPropertyEditor
          </editor-class>
          <named-value>
            <name>minValue</name>
            <expression>new Integer(0)</expression>
          </named-value>
          <named-value>
            <name>unsetValue</name>
            <expression>new Integer(Integer.MIN_VALUE)</expression>
         </named-value>       
        </attribute-extension>
      </attribute>

      <attribute>
        <attribute-name>disabledClass</attribute-name>
        <attribute-extension>
          <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.property.ChooseManyReferenceDataPropertyEditor
          </editor-class>
          <named-value>
            <name>referenceDataDefiner</name>
            <expression>
              com.sun.jsfcl.std.reference.ReferenceDataManager.STYLE_CLASSES
            </expression>
          </named-value>
        </attribute-extension>
      </attribute>
      
      <attribute>
        <attribute-name>enabledClass</attribute-name>
        <attribute-extension>
          <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.property.ChooseManyReferenceDataPropertyEditor
          </editor-class>
          <named-value>
            <name>referenceDataDefiner</name>
            <expression>
              com.sun.jsfcl.std.reference.ReferenceDataManager.STYLE_CLASSES
            </expression>
          </named-value>
        </attribute-extension>
      </attribute>
      
      <attribute>
        <attribute-name>layout</attribute-name>
        <attribute-extension>
          <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.property.ChooseOneReferenceDataPropertyEditor
          </editor-class>
          <named-value>
            <name>referenceDataDefiner</name>
            <expression>
              com.sun.jsfcl.std.reference.ReferenceDataManager.MANY_CHECKBOX_LAYOUT_STYLES
            </expression>
          </named-value>
        </attribute-extension>
      </attribute>

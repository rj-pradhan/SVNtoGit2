<!-- ========== Focus Attributes =========================================== -->


      <attribute>
        <attribute-name>accesskey</attribute-name>
        <attribute-extension>
          <category>ADVANCED</category>
        </attribute-extension>
      </attribute>

      <attribute>
        <attribute-name>onblur</attribute-name>
        <attribute-extension>
          <category>JAVASCRIPT</category>
          <editor-class>
            com.sun.jsfcl.std.JavaScriptPropertyEditor
          </editor-class>
        </attribute-extension>
      </attribute>

      <attribute>
        <attribute-name>onfocus</attribute-name>
        <attribute-extension>
          <category>JAVASCRIPT</category>
          <editor-class>
            com.sun.jsfcl.std.JavaScriptPropertyEditor
          </editor-class>
        </attribute-extension>
      </attribute>

      <attribute>
        <attribute-name>tabindex</attribute-name>
        <attribute-extension>
        <category>ADVANCED</category>
        <editor-class>
          com.sun.jsfcl.std.property.IntegerPropertyEditor
        </editor-class>
        <named-value>
          <name>minValue</name>
          <expression>new Integer(0)</expression>
        </named-value>
        <named-value>
          <name>maxValue</name>
          <expression>new Integer(Short.MAX_VALUE)</expression>
        </named-value>        
        </attribute-extension>
      </attribute>

      </property-extension>
    </property>

    <property>
      <property-name>longdesc</property-name>
      <property-extension>
          <category>APPEARANCE</category>
      </property-extension>
    </property>

    <property>
      <property-name>usemap</property-name>
      <property-extension>
          <category>ADVANCED</category>
          <editor-class>
            com.sun.jsfcl.std.property.ChooseOneReferenceDataPropertyEditor

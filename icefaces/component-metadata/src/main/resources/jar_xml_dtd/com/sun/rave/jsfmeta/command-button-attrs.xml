<!-- ========== Command+Button Attributes ================================== -->


    <attribute>
      <attribute-name>alt</attribute-name>
      <attribute-extension>
          <category>APPEARANCE</category>
      </attribute-extension>
    </attribute>

    <attribute>
      <attribute-name>image</attribute-name>
      <attribute-extension>
          <category>APPEARANCE</category>
          <editor-class>            
            com.sun.rave.propertyeditors.UrlPropertyEditor
          </editor-class>
      </attribute-extension>
    </attribute>

    <attribute>
      <attribute-name>type</attribute-name>
      <attribute-extension>
          <category>ADVANCED</category>
          <editor-class>
            com.sun.jsfcl.std.property.ChooseOneReferenceDataPropertyEditor
          </editor-class>
          <named-value>
            <name>referenceDataDefiner</name>
            <expression>
              com.sun.jsfcl.std.reference.ReferenceDataManager.COMMAND_BUTTON_TYPES
            </expression>
          </named-value>
      </attribute-extension>
    </attribute>

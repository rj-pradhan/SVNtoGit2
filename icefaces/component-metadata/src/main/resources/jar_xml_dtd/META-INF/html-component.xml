<?xml version="1.0" encoding="UTF-8"?>
<component>
  <description><![CDATA[
     This is an implementation of the HTML html tag. 
    ]]></description>

  <display-name>Html</display-name>

  <component-type>com.sun.rave.web.ui.Html</component-type>

  <property>
    <description>
        Defines the XML namespace attribute.  Default value is: 
        http://www.w3.org/1999/xhtml  	
      </description>
    <display-name>XML Namespace</display-name>
    <property-name>xmlns</property-name>
    <property-class>java.lang.String</property-class>
    <property-extension>
      <is-bindable>true</is-bindable>
      <default-value>"http://www.w3.org/1999/xhtml"</default-value>
    </property-extension>
  </property>

  <property>
    <description>
        Sets the language code for this document
      </description>
    <display-name>Lang</display-name>
    <property-name>lang</property-name>
    <property-class>java.lang.String</property-class>
    <property-extension>
      <is-bindable>true</is-bindable>
    </property-extension>
  </property>

  <component-extension>
    <base-component-type>com.sun.faces.Component</base-component-type>
    <component-family>com.sun.rave.web.ui.Html</component-family>
    <renderer-type>com.sun.rave.web.ui.Html</renderer-type>
  </component-extension>

</component>

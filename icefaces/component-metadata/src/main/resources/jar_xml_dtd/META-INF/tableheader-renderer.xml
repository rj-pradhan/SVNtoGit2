<?xml version="1.0" encoding="UTF-8"?>
<renderer>
  <description>
    <![CDATA[
The tableHeader component provides a layout mechanism for displaying headers.
    ]]>
  </description>

  <component-family>com.sun.rave.web.ui.TableHeader</component-family>
  <renderer-type>com.sun.rave.web.ui.TableHeader</renderer-type>

  <attribute>
    <attribute-name>axis</attribute-name>
    <attribute-extension>
      <tag-attribute>false</tag-attribute>
    </attribute-extension>
  </attribute>

  <attribute>
    <attribute-name>bgColor</attribute-name>
    <attribute-extension>
      <tag-attribute>false</tag-attribute>
    </attribute-extension>
  </attribute>

  <attribute>
    <attribute-name>char</attribute-name>
    <attribute-extension>
      <tag-attribute>false</tag-attribute>
    </attribute-extension>
  </attribute>

  <attribute>
    <attribute-name>charOff</attribute-name>
    <attribute-extension>
      <tag-attribute>false</tag-attribute>
    </attribute-extension>
  </attribute>

  <attribute>
    <attribute-name>colSpan</attribute-name>
    <attribute-extension>
      <tag-attribute>false</tag-attribute>
    </attribute-extension>
  </attribute>

  <attribute>
    <attribute-name>headers</attribute-name>
    <attribute-extension>
      <tag-attribute>false</tag-attribute>
    </attribute-extension>
  </attribute>

  <attribute>
    <attribute-name>rowSpan</attribute-name>
    <attribute-extension>
      <tag-attribute>false</tag-attribute>
    </attribute-extension>
  </attribute>

  &binding-attrs;
  &visible-attrs;

  <renderer-extension>
    <instance-name>tableHeader</instance-name>
    <!-- This component is not used as a JSP tag. -->
    <taglib-prefix>ui</taglib-prefix>
    <taglib-uri>http://www.sun.com/web/ui</taglib-uri>
  </renderer-extension>

</renderer>

<?xml version="1.0" encoding="UTF-8"?>
<renderer>
	<description>
		<![CDATA[<p>Renders a UIComponent that represents a single column of data within a parent <code>UIData</code> component.</p>]]>
	</description>

	<component-family>javax.faces.Column</component-family>
	<renderer-type>null</renderer-type>

	<renderer-extension>
		<instance-name>column</instance-name>
		<is-container>true</is-container>
		<renders-children>true</renders-children>
		<tag-name>column</tag-name>
		<taglib-prefix>@taglib-prefix@</taglib-prefix>
		<taglib-uri>@taglib-uri@</taglib-uri>
	</renderer-extension>

</renderer>
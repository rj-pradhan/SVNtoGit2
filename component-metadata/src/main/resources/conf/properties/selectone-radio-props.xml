<?xml encoding='UTF-8'?>

      <property>
        <property-name>border</property-name>
        <property-extension>
          <category>APPEARANCE</category>
          <editor-class>
            &integerEditor;
          </editor-class>
          <named-value>
            <name>minValue</name>
            <expression>new Integer(0)</expression>
          </named-value>
          <named-value>
            <name>unsetValue</name>
            <expression>new Integer(Integer.MIN_VALUE)</expression>
         </named-value>       
        </property-extension>
      </property>

      <property>
        <property-name>disabledClass</property-name>
        <property-extension>
          <category>APPEARANCE</category>
 		<editor-class>
			&stringEditor;
		</editor-class>
        </property-extension>
      </property>
      
      <property>
        <property-name>enabledClass</property-name>
        <property-extension>
          <category>APPEARANCE</category>
		<editor-class>
			&stringEditor;
		</editor-class>
        </property-extension>
      </property>
      
      <property>
        <property-name>layout</property-name>
        <property-extension>
          <category>APPEARANCE</category>
          <editor-class>
            &chooseOneReferenceDataEditor;
          </editor-class>
          <named-value>
            <name>referenceDataDefiner</name>
            <expression>
              com.sun.jsfcl.std.reference.ReferenceDataManager.MANY_CHECKBOX_LAYOUT_STYLES
            </expression>
          </named-value>
        </property-extension>
      </property>

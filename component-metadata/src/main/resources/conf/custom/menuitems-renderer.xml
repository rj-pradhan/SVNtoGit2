<?xml version="1.0" encoding="UTF-8"?>
<renderer>
	<description>
           This is the submenu component to use if you want to supply a (potentially) dynamic 
            heirarchy of menuItems.
	</description>
	<component-family>com.icesoft.faces.MenuNodes</component-family>

	<renderer-type>com.icesoft.faces.View</renderer-type>

	<renderer-extension>
		<instance-name>menuItems</instance-name>
		<is-container>true</is-container>
		<renders-children>true</renders-children>
		<tag-name>menuItems</tag-name>
		<taglib-prefix>@taglib-prefix@</taglib-prefix>
		<taglib-uri>@taglib-uri@</taglib-uri>
	</renderer-extension>

</renderer>

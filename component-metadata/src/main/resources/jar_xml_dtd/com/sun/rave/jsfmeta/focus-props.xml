<!-- ========== Focus Properties =========================================== -->


      <property>
        <property-name>accesskey</property-name>
        <property-extension>
          <category>ADVANCED</category>
        </property-extension>
      </property>

      <property>
        <property-name>onblur</property-name>
        <property-extension>
          <category>JAVASCRIPT</category>
          <editor-class>
            com.sun.jsfcl.std.JavaScriptPropertyEditor
          </editor-class>
        </property-extension>
      </property>

      <property>
        <property-name>onfocus</property-name>
        <property-extension>
          <category>JAVASCRIPT</category>
          <editor-class>
            com.sun.jsfcl.std.JavaScriptPropertyEditor
          </editor-class>
        </property-extension>
      </property>

      <property>
        <property-name>tabindex</property-name>
        <property-extension>
        <category>ADVANCED</category>
        <editor-class>
          com.sun.jsfcl.std.property.IntegerPropertyEditor
        </editor-class>
        <named-value>
          <name>minValue</name>
          <expression>new Integer(0)</expression>
        </named-value>
        <named-value>
          <name>maxValue</name>
          <expression>new Integer(Short.MAX_VALUE)</expression>
        </named-value>        
        </property-extension>
      </property>

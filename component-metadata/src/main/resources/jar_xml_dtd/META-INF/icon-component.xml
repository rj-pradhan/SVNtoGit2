<?xml version="1.0" encoding="UTF-8"?>
<component>
  <description><![CDATA[
      Render an img element to display the theme-specific image, rendering
      the description property (if any) as the value of the alt attribute.
    ]]></description>

  <display-name>Image</display-name>

  <component-type>com.sun.rave.web.ui.Icon</component-type>

  <component-extension>
    <base-component-type>com.sun.rave.web.ui.Image</base-component-type>
    <component-family>com.sun.rave.web.ui.Icon</component-family>
    <renderer-type>com.sun.rave.web.ui.Icon</renderer-type>
  </component-extension>

</component>
